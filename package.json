{
  "name": "versatile-npm",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngseke/versatile-npm"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint . --ext .ts,.js,.cjs,.vue",
    "lint:fix": "npx eslint --fix . --ext .ts,.js,.cjs,.vue",
    "bump": "pnpm run lint && pnpm run test run && vue-tsc && node ./scripts/bump.js",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "nanoid": "^5.0.3",
    "vue": "^3.3.8",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.4.3"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^1.0.14",
    "@types/chrome": "^0.0.252",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.2",
    "deepmerge": "^4.3.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "is-plain-obj": "^4.1.0",
    "jsdom": "^22.1.0",
    "open": "^9.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "url-join": "^5.0.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
